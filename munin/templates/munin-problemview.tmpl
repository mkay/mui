                                        <img class="card-img-top crit" src="<TMPL_VAR NAME="CIMGWEEK">" alt="<TMPL_VAR ESCAPE="HTML" NAME="LABEL">" />
                                    <TMPL_IF NAME="URL"></a></TMPL_IF>
                                </div>
                            </div>
                        </div>
                        </TMPL_LOOP>
                    </div>
                    <TMPL_ELSE>
                    <div class="problem mt-4" id="critical">
                        <h5 class="mt-0 mb-1 bg-light text-dark py-1 px-3 shadow-sm"><i class="fas fa-fw fa-exclamation-triangle"></i> No critical problems</h5>
                    </div>
