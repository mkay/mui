<TMPL_INCLUDE NAME="partial/head.tmpl">
    <body>
        <TMPL_INCLUDE NAME="partial/navigation.tmpl">
        <main class="container-fluid">
        	    <div class="row h-100">
        	        <aside class="d-none d-md-block col-md-5 col-lg-4 col-xl-3 bg-dark text-light">
        	            <TMPL_INCLUDE NAME="partial/sidebar.tmpl">
        	        </aside>
        	        <article class="col-md-7 col-lg-8 col-xl-9 col-content">

                    <h2 class="node-heading mb-3">Problems</h2>

                    <TMPL_IF NAME="CRITICAL">
                    <div class="problem mt-4" id="critical">
                        <h5 class="mt-0 mb-1 bg-danger text-light py-1 px-3 shadow-sm"><i class="fas fa-fw fa-exclamation-triangle"></i> Critical</h5>
                        <TMPL_LOOP NAME="CRITICAL">
                        <div class="row mt-3 px-3">
                            <div class="col-sm-12">
                                <h5 class="card-title card-category"><a class="nodetitle nt" href="<TMPL_VAR NAME="NODEURL">"><TMPL_VAR ESCAPE="HTML" NAME="NODENAME"></a></h5>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-12 col-lg-6">
                                <div class="card-body pt-1">
                            			<TMPL_IF NAME="URL"><a href="<TMPL_VAR NAME="URL">"></TMPL_IF>
                            			<TMPL_VAR ESCAPE="HTML" NAME="LABEL"> / Day
                            			<TMPL_IF NAME="URL"></a></TMPL_IF>
                                </div>                          
                                <div class="card mb-4 shadow-sm">
                                    <TMPL_IF NAME="URL"><a href="<TMPL_VAR NAME="URL">"></TMPL_IF>
                                        <img class="card-img-top crit" src="<TMPL_VAR NAME="CIMGDAY">" alt="<TMPL_VAR ESCAPE="HTML" NAME="LABEL">" />
                                    <TMPL_IF NAME="URL"></a></TMPL_IF>
                                </div>
                            </div>
